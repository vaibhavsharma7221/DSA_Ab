//Factorial of a Number using Recursion

#include<iostream>
using namespace std;

int fact(int n)
{
    if(n==0)
        return 1;
    else
        return fact(n-1)*n;
}

int loopf(int n)
{
    int f=1;
    for(int i=1;i<=n;i++)
    {
        f=f*i;
    }
    return f;
}

int main()
{
    int n;
    cin>>n;
    
    cout<<fact(n)<<"\n";
    cout<<loopf(n);
    
    return 0;
}
